{
    "welcomeFile": "/index.html",
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/resources/(.*)$",
            "target": "/resources/$1",
            "authenticationType": "none",
            "destination": "ui5"
        },
        {
            "source": "^/library/(.*)$",
            "target": "/library/$1",
            "destination": "lib_approuter_app_srv",
            "authenticationType": "xsuaa",
            "csrfProtection": false
        },
        {
            "source": "^/library/(.*)$",
            "target": "/library/$1",
            "destination": "lib_approuter_app_srv",
            "authenticationType": "xsuaa",
            "csrfProtection": false
        },
        {
            "source": "^(.*)$",
            "target": "$1",
            "service": "html5-apps-repo-rt",
            "authenticationType": "xsuaa"
        }
    ]
}